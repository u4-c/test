        local function PlayerAdd(Name)
            local TextLabel = Instance.new("TextButton")
    
            TextLabel.Parent = List
            TextLabel.BackgroundColor3 = Color3.fromRGB(66, 66, 66)
            TextLabel.Position = UDim2.new(0.0142045459, 0, 0, 0)
            TextLabel.Size = UDim2.new(0, 173, 0, 30)
            TextLabel.Font = Enum.Font.SourceSansBold
            TextLabel.LineHeight = 0.800
            TextLabel.Text = Name
            TextLabel.Name = Name
            TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
            TextLabel.TextSize = 16.000
            TextLabel.MouseButton1Click:Connect(function()
                for i,v in pairs(game.Players:GetChildren()) do
                    if v.Name == Name or v.UserId == Name then
                        Player.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                    end
                end
            end)
    
            local contentsize = TextLabel.Parent:FindFirstChildOfClass('UIListLayout').AbsoluteContentSize
            TextLabel.Parent.CanvasSize = UDim2.new(0,contentsize.X,0,contentsize.Y)
        end
        
        DupersInfo.MouseButton1Click:Connect(function()
            SetUpFrame.Visible = false
            AccountInfos.Visible = true
        end)
        SetUp.MouseButton1Click:Connect(function()
            SetUpFrame.Visible = true
            AccountInfos.Visible = false
        end)
        
        local formatNumber = (function (n)
            n = tostring(n)
            return n:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
        end)
        
        local function AddDuper(Name, Cash, ID)
            local PlayersName = Instance.new("Frame")
            local PlayersImage = Instance.new("ImageLabel")
            local NameCash = Instance.new("TextLabel")
            
            PlayersName.Name = Name
            PlayersName.Parent = ScrollingFrame
            PlayersName.BackgroundColor3 = Color3.fromRGB(122, 122, 122)
            PlayersName.Size = UDim2.new(0, 177, 0, 47)
            
            PlayersImage.Name = Name
            PlayersImage.Parent = PlayersName
            PlayersImage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            PlayersImage.BackgroundTransparency = 1.000
            PlayersImage.BorderSizePixel = 0
            PlayersImage.Size = UDim2.new(0, 44, 0, 44)
            PlayersImage.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
            
            NameCash.Name = "NameCash"
            NameCash.Parent = PlayersName
            NameCash.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            NameCash.BackgroundTransparency = 1.000
            NameCash.Position = UDim2.new(0.248587564, 0, 0, 0)
            NameCash.Size = UDim2.new(0, 133, 0, 44)
            NameCash.Font = Enum.Font.SourceSansSemibold
            NameCash.Text = Name .. '\n$' ..  formatNumber(Cash)
            NameCash.TextColor3 = Color3.fromRGB(0, 0, 0)
            NameCash.TextSize = 14.000
            NameCash.TextWrapped = true
            
            coroutine.resume(coroutine.create(function()
                local TargetId = ID
                local ThumbType = Enum.ThumbnailType.HeadShot
                local ThumbSize = Enum.ThumbnailSize.Size420x420
                local Content, IsReady = game:GetService('Players'):GetUserThumbnailAsync(TargetId, ThumbType, ThumbSize)
                PlayersImage.Image = Content
            end))
            
        end
        
        TextButton.MouseButton1Click:Connect(function() --\\ Load all people 
            for i,v in pairs(ScrollingFrame:GetChildren()) do 
                if v:IsA('Frame') then
                    v:Destroy()
                end
            end
            for _, Plr in pairs(game.Players:GetChildren()) do
                if table.find(DUPERS, Plr.Name) or table.find(DUPERS, tostring(Plr.UserId)) then
                    local s,e = pcall(function()
                        AddDuper(Plr.Name, Plr.DataFolder.Currency.Value, Plr.UserId)
                    end) if e then print(e) end
                end
            end
        end)
        
        TextButton_2.Parent = MainList
        TextButton_2.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
        TextButton_2.BorderSizePixel = 0
        TextButton_2.Position = UDim2.new(-0.0146642029, 0, 0.881712139, 0)
        TextButton_2.Size = UDim2.new(0, 176, 0, 39)
        TextButton_2.Font = Enum.Font.GothamBold
        TextButton_2.Text = "Save List"
        TextButton_2.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton_2.TextSize = 14.000
        TextButton_2.Visible = false
        
        ImageButton.MouseButton1Click:Connect(function()
            if HelpFrame.Visible == false then
                HelpFrame.Visible = true
            else
                HelpFrame.Visible = false
            end
        end)
        
        local function Update()
            for i,v in pairs(List:GetChildren()) do
               if v:IsA('TextLabel') then v:Destroy() end 
            end
            local s,e = pcall(function()
            for i,v in pairs(DUPERS) do
                PlayerAdd(v)
            end
            end) if e then print(e) end
        end
        Update()
        Add.MouseButton1Click:Connect(function()
            local PlayerInfo = table.find(DUPERS, Textbox.Text)
            if not PlayerInfo and #Textbox.Text > 0 then
                table.insert(DUPERS, Textbox.Text)
                print('successfully added' .. Textbox.Text)
                Update()
            end
        end)
            
        Remove.MouseButton1Click:Connect(function()
            local PlayerInfo = table.find(DUPERS, Textbox.Text)
            if PlayerInfo and #Textbox.Text > 0 then
                table.remove(DUPERS, PlayerInfo)
                print('successfully removed' .. Textbox.Text)
                Update()
            end
        end)
            
        CrasherUsername.Text = tostring(DATA.Crasher)
        CrashAtCash.Text = tostring(DATA.CrashAt)
        CrashAfterSeconds.Text = tostring(DATA.CrashAfter)
        Fps.Text = tostring(DATA.FPS)
        
        if DATA.GFX == 'NormalGFX' then
            DATA.GFX = 'NormalGFX'
            NormalGFX.Enabled.Visible = true
            NormalGFX.Disabled.Visible = false
            ReallyLowGFX.Disabled.Visible = true
            ReallyLowGFX.Enabled.Visible = false
            LowGFX.Disabled.Visible = true
            LowGFX.Enabled.Visible = false 
        elseif DATA.GFX == 'LowGFX' then
            DATA.GFX = 'LowGFX'
            NormalGFX.Enabled.Visible = false
            NormalGFX.Disabled.Visible = true
            ReallyLowGFX.Disabled.Visible = true
            ReallyLowGFX.Enabled.Visible = false
            LowGFX.Disabled.Visible = false
            LowGFX.Enabled.Visible = true
        else
            DATA.GFX = 'ReallyLowGFX'
            NormalGFX.Enabled.Visible = false
            NormalGFX.Disabled.Visible = true
            ReallyLowGFX.Disabled.Visible = false
            ReallyLowGFX.Enabled.Visible = true
            LowGFX.Disabled.Visible = true
            LowGFX.Enabled.Visible = false
        end
        
        local s,e = pcall(function()
            NormalGFX.MouseButton1Click:Connect(function()
                DATA.GFX = 'NormalGFX'
                NormalGFX.Enabled.Visible = true
                NormalGFX.Disabled.Visible = false
                ReallyLowGFX.Disabled.Visible = true
                ReallyLowGFX.Enabled.Visible = false
                LowGFX.Disabled.Visible = true
                LowGFX.Enabled.Visible = false
            end)
            LowGFX.MouseButton1Click:Connect(function()
                DATA.GFX = 'LowGFX'
                NormalGFX.Enabled.Visible = false
                NormalGFX.Disabled.Visible = true
                ReallyLowGFX.Disabled.Visible = true
                ReallyLowGFX.Enabled.Visible = false
                LowGFX.Disabled.Visible = false
                LowGFX.Enabled.Visible = true
            end)
            ReallyLowGFX.MouseButton1Click:Connect(function()
                DATA.GFX = 'ReallyLowGFX'
                NormalGFX.Enabled.Visible = false
                NormalGFX.Disabled.Visible = true
                ReallyLowGFX.Disabled.Visible = false
                ReallyLowGFX.Enabled.Visible = true
                LowGFX.Disabled.Visible = true
                LowGFX.Enabled.Visible = false
            end)
    
            ConfirmSettings.MouseButton1Click:Connect(function()
                local s,e = pcall(function()
                    if #CrashAtCash.Text > 0 then
                        writefile("DupeData/CrashAtCash.txt", tostring(CrashAtCash.Text))
                    else
                        writefile("DupeData/CrashAtCash.txt", '5000')
                    end
                    if #CrasherUsername.Text > 0 then
                        writefile("DupeData/Crasher.txt", tostring(CrasherUsername.Text))
                    else
                        writefile("DupeData/Crasher.txt", tostring('nil\nil'))
                    end
                    if #CrashAfterSeconds.Text > 0 then
                        writefile("DupeData/CrashAfter.txt", tostring(CrashAfterSeconds.Text)) 
                    else
                        writefile("DupeData/CrashAfter.txt", tostring('999999999999')) 
                    end
                    writefile("DupeData/GFX.txt", DATA.GFX) 
                    if #Fps.Text > 0 then
                        writefile("DupeData/FPS.txt", tostring(Fps.Text)) 
                    else
                        writefile("DupeData/FPS.txt", tostring('15')) 
                    end
                    writefile("DupeData/DuperList.txt", table.concat(DUPERS, '/')) 
                    print('Data Saved,')
                    announce('Saved Data Successfully', 'accounts that join will apply these settings', 5)
                end) if e then print('Error saving data : ' .. e) end
            end)
        end) if e then print(e) end
    end
end
